programa
{
	
	funcao inicio()
	{
		inteiro n1, n2

		escreva("Digite o número n1: ")
		leia(n1)

		escreva("Digite o número n2: ")
		leia(n2)

		se(n1 > n2) {
			escreva("O maior número entre eles é: ", n1)
		}	senao {
			escreva("O maior número entre eles é: ", n2)
		}
		
		
		
	}
}