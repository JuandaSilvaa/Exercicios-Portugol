programa
{
  funcao inicio()
  {
    inteiro vetor[5]

    para(inteiro j = 0; j < 5; j++)
    {
      escreva("Escreva o ", j + 1,"º" ," número: ")
      leia(vetor[j])
      
    }
    para(inteiro i = 0; i < 5; i++)
    {
    escreva("\n","\t", vetor[i], " ", vetor[i])
    }

  }

}
